# GraphQL Configuration
# Copy this file to .env.local and update with your actual URLs

# Your GraphQL endpoint URL
NEXT_PUBLIC_GRAPHQL_URL=https://your-graphql-endpoint.com/graphql/

# Backend URL
NEXT_PUBLIC_BACKEND_URL=na

# Example for local development with ngrok:
# NEXT_PUBLIC_GRAPHQL_URL=https://1af83ff1d893.ngrok-free.app/graphql/

# Example for local development:
# NEXT_PUBLIC_GRAPHQL_URL=http://localhost:4000/graphql/

# Example for production:
# NEXT_PUBLIC_GRAPHQL_URL=https://api.deyor.com/graphql/
